{
    "name": "@contredanse/mfts",
    "private": true,
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "cross-env NODE_ENV=production BABEL_ENV=production STATIC_COMPRESS=true webpack --mode production --config webpack.prod.js",
        "serve": "npx serve ./dist/public",
        "now-build": "cross-env NODE_ENV=production BABEL_ENV=production STATIC_COMPRESS=false webpack --mode production --config webpack.prod.js",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack.dev.js",
        "clean": "run-p clean:*",
        "clean:build": "rimraf build",
        "clean:dist": "rimraf dist",
        "typecheck": "tsc -p . --noEmit",
        "lint": "tslint --project './tsconfig.json'",
        "lint:fix": "tslint --project './tsconfig.json' --fix",
        "prettier": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json}\" \"./server/**/*.{ts,tsx,scss,js,json}\" --write \"./*.{js,json}\"",
        "test": "jest --config jest.config.js",
        "test:coverage": "jest --config jest.config.js --coverage",
        "test:watch": "jest --config jest.config.js --watch",
        "test:update": "jest --config jest.config.js -u",
        "source-map-explorer": "source-map-explorer ./dist/main.*.js ./dist/main.*.js.map",
        "stats": "yarn build && webpack-bundle-analyzer dist/.webpack-stats.json",
        "deduplicate": "yarn yarn-deduplicate"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "last 2 versions",
        "chrome >= 71",
        "firefox >= 68",
        "safari >= 11",
        "edge >= 16",
        "not dead",
        "not ie <= 12",
        "not op_mini all"
    ],
